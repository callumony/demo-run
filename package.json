{
  "name": "omnipotent",
  "version": "1.0.0",
  "description": "AI Agent Boilerplate",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:web": "concurrently \"npm run server\" \"npm run client\"",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "build:electron": "npm run build && electron-builder",
    "setup": "npm install && cd client && npm install",
    "build:web": "cd client && npm run build",
    "start:prod": "node server/index.js"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.23.0",
    "archiver": "^7.0.1",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "node-pty": "^1.1.0",
    "openai": "^4.28.0",
    "pdf-parse": "^2.4.5",
    "tesseract.js": "^7.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.19.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.0.3"
  },
  "build": {
    "appId": "com.omnipotent.agent",
    "productName": "OMNIPOTENT",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "electron/**/*",
      "server/**/*",
      "client/dist/**/*",
      "data/**/*",
      "package.json",
      ".env"
    ],
    "win": {
      "target": "nsis",
      "icon": "electron/icons/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "",
  "license": "MIT"
}
